                                  {% endif %}
                                  {% if function('in_array', 'author', fields.element_modified.modified_elements) and post.modified_author %}
                                    {% if fields.link_items != true and fields.element_modified.link_author == true %}
                                      <a class="posts-loop--post-modified--link posts-loop--post-modified--author" href="{{ post.modified_author.link }}">
                                    {% else %}
                                      <span class="posts-loop--post-modified--author">
                                    {% endif %}
                                    {{ post.modified_author.name }}
                                    {% if fields.link_items != true and fields.element_modified.link_author == true %}
                                      </a>
                                    {% else %}
                                      </span>
                                    {% endif %}
