                                  {% else %}
                                    </span>
                                  {% endif %}
                                {% elseif author_element.acf_fc_layout == 'name' and author.name %}
                                  {% if author_element.label %}
                                    <span class="posts-loop--post-author--label">{{ author_element.label }}</span>
