                                        <a class="posts-loop--post-author--link posts-loop--post-author--name" href="{{ author.link }}">
                                      {% else %}
                                        <span class="posts-loop--post-author--name">
                                      {% endif %}
                                      {{ author.name }}
                                      {% if fields.link_items != true and fields.element_author.link_author == true %}
                                        </a>
                                      {% else %}
                                        </span>
                                      {% endif %}
