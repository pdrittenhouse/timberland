                        {{ post.type.labels.singular_name }}
                      {% elseif element.name_format == 'menu_name' %}
                        {{ post.type.labels.menu_name }}
                      {% elseif element.name_format == 'name_admin_bar' %}
                        {{ post.type.labels.name_admin_bar }}
